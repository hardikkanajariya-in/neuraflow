  "automations": [
    {
      "id": "auto-001",
      "name": "New User Welcome Sequence",
      "description": "Triggers when a new user signs up, sends verification email, creates CRM contact, and starts drip campaign.",
      "projectId": "proj-001",
      "enabled": true,
      "trigger": { "id": "s1", "type": "trigger", "label": "Webhook: New Signup", "config": "POST /api/webhooks/signup", "icon": "Webhook" },
      "conditions": [
        { "id": "s2", "type": "condition", "label": "Email Verified", "config": "user.emailVerified === true", "icon": "CheckCircle" },
        { "id": "s3", "type": "condition", "label": "Plan Type Check", "config": "user.plan !== 'free'", "icon": "Filter" }
      ],
      "actions": [
        { "id": "s4", "type": "action", "label": "Create CRM Contact", "config": "Salesforce → Contacts.create()", "icon": "UserPlus" },
        { "id": "s5", "type": "action", "label": "Send Welcome Email", "config": "SendGrid → template: welcome-v2", "icon": "Mail" },
        { "id": "s6", "type": "action", "label": "Notify Slack", "config": "#new-signups → formatted message", "icon": "MessageSquare" }
      ],
      "lastRunAt": "2026-02-28T09:45:00Z",
      "runCount": 4200
    },
    {
      "id": "auto-002",
      "name": "CRM Data Sync",
      "description": "Bi-directional sync between Salesforce and HubSpot with conflict resolution and field mapping.",
      "projectId": "proj-002",
      "enabled": true,
      "trigger": { "id": "s1", "type": "trigger", "label": "Schedule: Every 15 min", "config": "cron: */15 * * * *", "icon": "Clock" },
      "conditions": [
        { "id": "s2", "type": "condition", "label": "Records Modified", "config": "lastModified > lastSyncTimestamp", "icon": "RefreshCw" }
      ],
      "actions": [
        { "id": "s3", "type": "action", "label": "Fetch Changed Records", "config": "Salesforce → query modified contacts", "icon": "Download" },
        { "id": "s4", "type": "action", "label": "Transform & Map Fields", "config": "Apply field mapping rules v3", "icon": "ArrowRightLeft" },
        { "id": "s5", "type": "action", "label": "Upsert to HubSpot", "config": "HubSpot → contacts.batchUpsert()", "icon": "Upload" }
      ],
      "lastRunAt": "2026-02-28T10:00:00Z",
      "runCount": 18500
    },
    {
      "id": "auto-003",
      "name": "Model Drift Detector",
      "description": "Monitors production ML model performance metrics and triggers retraining when accuracy drops below threshold.",
      "projectId": "proj-003",
      "enabled": true,
      "trigger": { "id": "s1", "type": "trigger", "label": "Schedule: Daily 2 AM", "config": "cron: 0 2 * * *", "icon": "Clock" },
      "conditions": [
        { "id": "s2", "type": "condition", "label": "Accuracy Check", "config": "model.accuracy < 0.95", "icon": "TrendingDown" },
        { "id": "s3", "type": "condition", "label": "Data Volume Check", "config": "newSamples > 10000", "icon": "Database" }
      ],
      "actions": [
        { "id": "s4", "type": "action", "label": "Launch Retraining Job", "config": "SageMaker → training.start(config_v2)", "icon": "Cpu" },
        { "id": "s5", "type": "action", "label": "Notify ML Team", "config": "#ml-alerts → drift report", "icon": "MessageSquare" }
      ],
      "lastRunAt": "2026-02-28T02:00:00Z",
      "runCount": 420
    },
    {
      "id": "auto-004",
      "name": "Invoice OCR & Routing",
      "description": "Extracts data from uploaded invoices using OCR, validates against POs, and routes for approval.",
      "projectId": "proj-004",
      "enabled": true,
      "trigger": { "id": "s1", "type": "trigger", "label": "File Upload: S3 Bucket", "config": "s3://invoices/incoming/*", "icon": "Upload" },
      "conditions": [
        { "id": "s2", "type": "condition", "label": "File Type Check", "config": "file.type in ['pdf', 'png', 'jpg']", "icon": "FileCheck" }
      ],
      "actions": [
        { "id": "s3", "type": "action", "label": "OCR Extraction", "config": "Textract → extract fields", "icon": "ScanText" },
        { "id": "s4", "type": "action", "label": "PO Validation", "config": "Match against PO database", "icon": "CheckSquare" },
        { "id": "s5", "type": "action", "label": "Route for Approval", "config": "If amount > $5K → VP else → Manager", "icon": "GitBranch" }
      ],
      "lastRunAt": "2026-02-27T16:30:00Z",
      "runCount": 6800
    },
    {
      "id": "auto-005",
      "name": "Security Alert Classifier",
      "description": "AI-powered classification of security alerts with severity scoring and automatic ticket creation.",
      "projectId": "proj-005",
      "enabled": false,
      "trigger": { "id": "s1", "type": "trigger", "label": "Event: SIEM Alert", "config": "Splunk → alert stream", "icon": "ShieldAlert" },
      "conditions": [
        { "id": "s2", "type": "condition", "label": "Severity > Medium", "config": "alert.severity >= 3", "icon": "AlertTriangle" }
      ],
      "actions": [
        { "id": "s3", "type": "action", "label": "AI Classification", "config": "GPT-4 → classify + score", "icon": "Brain" },
        { "id": "s4", "type": "action", "label": "Create Jira Ticket", "config": "Jira → security-incidents project", "icon": "Ticket" },
        { "id": "s5", "type": "action", "label": "Page On-Call", "config": "PagerDuty → if critical", "icon": "Phone" }
      ],
      "lastRunAt": "2026-02-20T11:45:00Z",
      "runCount": 34800
    },
    {
      "id": "auto-006",
      "name": "Slack Command Router",
      "description": "Intelligent routing of Slack slash commands to appropriate automations and response handlers.",
      "projectId": "proj-007",
      "enabled": true,
      "trigger": { "id": "s1", "type": "trigger", "label": "Slack: Slash Command", "config": "/neuraflow [action] [params]", "icon": "MessageSquare" },
      "conditions": [
        { "id": "s2", "type": "condition", "label": "Auth Check", "config": "user.role >= 'member'", "icon": "Lock" },
        { "id": "s3", "type": "condition", "label": "Rate Limit", "config": "requests < 10/min per user", "icon": "Timer" }
      ],
      "actions": [
        { "id": "s4", "type": "action", "label": "Parse Command", "config": "NLP → extract intent + entities", "icon": "Brain" },
        { "id": "s5", "type": "action", "label": "Execute Action", "config": "Route to matching automation", "icon": "Play" },
        { "id": "s6", "type": "action", "label": "Send Response", "config": "Slack → ephemeral or channel reply", "icon": "Send" }
      ],
      "lastRunAt": "2026-02-28T10:12:00Z",
      "runCount": 52400
    }
  ],
